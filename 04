#include <stdio.h>
#include <stdlib.h>

int main()
{
    float sp, rj, mg, es, out, total, psp,prj, pmg, pes,pout;

    scanf("%f %f %f %f %f", &sp, &rj, &mg, &es, &out);

    total = (sp + rj) + (mg +es) + out;
    psp = (sp/total) * 100;
    prj = (rj/total) * 100;
    pmg = (mg/total) * 100;
    pes = (es/total) * 100;
    pout = (out/total) * 100;

    printf("SP = %.2f %%\nRJ = %.2f %%\nMG = %.2f %%\nES = %.2f %%\nOUTROS = %.2f %%\n",psp,prj,pmg,pes,pout);

    return 0;
}
